<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lcng.testphonegap1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>TestPhoneGap1</name>
    <description>
    1Hello World sample application that responds to the deviceready event.
  </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
    Lcng
  </author>
    <content src="index.html" />
    <config-file mode="merge" parent="/manifest" platform="android">
        <application android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" />
    </config-file>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
